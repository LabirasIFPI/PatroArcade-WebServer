{
    "version": 2,
    "builds": [
     {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": { 
        "includeFiles": ["dist/**"],
        "tsconfig": "tsconfig.json"
    }
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "dist/server.js"
     }
    ]
   }